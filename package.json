    "name": "flyteconsole",
    "version": "0.0.24",
    "description": "The web UI for the Flyte platform",
    "repository": {
        "type": "git",
        "url": "http://github.com/lyft/flyteconsole.git"
    },
    "main": "index.js",
    "scripts": {
        "build:prod":
            "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' NODE_ENV=production webpack --config webpack.config.ts --mode=production",
        "build":
            "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' webpack --config webpack.config.ts --mode=development",
        "clean": "rm -rf dist",
        "lint": "tslint --project .",
        "lint-fix":
            "tslint --fix --project . && prettier --write './**/*.{js,json}'",
        "lint:hooks": "eslint \"src/**/*.{ts,tsx}\"",
        "mockApi": "node ./mockApi",
        "prebuild": "yarn run clean",
        "precommit": "lint-staged",
        "start": "node -r dotenv/config index.js",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "tdd": "yarn run test --watch --verbose false",
        "test": "NODE_ENV=test jest --config=jest.config.js"
    },
    "keywords": ["react", "lyft"],
    "author": "Lyft <flyte-eng@lyft.com>",
    "license": "Apache-2.0",
    "lint-staged": {
        "*.{ts,tsx}": ["eslint", "tslint --fix", "git add"],
        "*.{js,json}": ["prettier --write", "git add"]
    },
